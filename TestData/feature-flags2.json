{
  "schemaVersion": "2.0.0",
  "featureFlags": [
    {
      "id": "user_prompt",
      "enabled": true,
      "variants": [
        {
          "name": "professional",
          "configuration_value": "professional"
        },
        {
          "name": "tactful",
          "configuration_value": "tactful"
        }
      ],
      "allocation": {
        "percentile": [
          {
            "variant": "professional",
            "from": 0,
            "to": 50
          },
          {
            "variant": "tactful",
            "from": 50,
            "to": 100
          }
        ],
        "default_when_enabled": "professional",
        "default_when_disabled": "professional"
      },
      "telemetry": {
        "enabled": true
      }
    }
  ]
}
